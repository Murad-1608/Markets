    <Page Remove="Enums\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Images\Back_Button_Icon.jpg" />
    <None Remove="Images\DeleteIcon.png" />
    <None Remove="Images\kontakthome.png" />
    <None Remove="Images\menuIcon.png" />
    <None Remove="Images\pen-icon-png-0.jpg" />
    <None Remove="Images\UpperButton.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\menuIcon.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\pen-icon-png-0.jpg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SqlClient" Version="4.8.3" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\Back_Button_Icon.jpg" />
